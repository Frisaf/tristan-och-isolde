{% extends "layout.njk" %}

{% block content %}

<main>
    <h1>{{ title }}</h1>
    <p><em>{{ message }}</em></p>
    
    {% for choice in choices %}
        <p><a href="/story_page/{{ choice.target }}">{{ choice.text }}</a></p>
    {% else %}
        <p>You reached the end!</p>
    {% endfor %}
</main>

{% endblock %}